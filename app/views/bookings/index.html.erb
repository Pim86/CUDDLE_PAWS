<h1>Bookings</h1>
<% @bookings.each do |booking| %>
  <%= booking.pet.name %>
  <%= booking.user.full_name %>
  <%= booking.start_date_time %>
  <%= booking.end_date_time %>

  <%= link_to 'Show', booking_path(booking) %>
<% end %>

<div class="text-center mt-3">
  <button class="btn btn-flat">
    <%= link_to 'Home', root_path %>
  </button>
</div>
